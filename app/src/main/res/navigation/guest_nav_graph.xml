<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/guest_nav_graph"
    app:startDestination="@id/guestHomeFragment">
    <fragment
        android:id="@+id/guestHomeFragment"
        android:name="com.example.esmmobile.ui.guestScreen.GuestHomeFragment"
        android:label="fragment_guest_home"
        tools:layout="@layout/fragment_guest_home" >
        <action
            android:id="@+id/action_guestHomeFragment_to_applyNowFragment"
            app:destination="@id/applyNowFragment"
            app:enterAnim="@anim/enter"
            app:exitAnim="@anim/exit"
            app:popEnterAnim="@anim/pop_enter"
            app:popExitAnim="@anim/pop_exit" />
        <action
            android:id="@+id/action_guestHomeFragment_to_guestUndergraduateCoursesFragment"
            app:destination="@id/guestUndergraduateCoursesFragment"
            app:enterAnim="@anim/enter"
            app:exitAnim="@anim/exit"
            app:popEnterAnim="@anim/pop_enter"
            app:popExitAnim="@anim/pop_exit" />
        <action
            android:id="@+id/action_guestHomeFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/enter"
            app:exitAnim="@anim/exit"
            app:popEnterAnim="@anim/pop_enter"
            app:popExitAnim="@anim/pop_exit" />
    </fragment>
    <fragment
        android:id="@+id/applyNowFragment"
        android:name="com.example.esmmobile.ui.guestScreen.ApplyNowFragment"
        android:label="fragment_apply_now"
        tools:layout="@layout/fragment_apply_now" />
    <fragment
        android:id="@+id/guestUndergraduateCoursesFragment"
        android:name="com.example.esmmobile.ui.guestScreen.GuestUndergraduateCoursesFragment"
        android:label="fragment_guest_undergraduate_courses"
        tools:layout="@layout/fragment_guest_undergraduate_courses" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.esmmobile.ui.guestScreen.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" />
</navigation>